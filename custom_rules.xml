<?xml version="1.0" encoding="UTF-8"?>
<project name="nv-websocket-client-rules" default="help">

  <!-- A rule to generate the JAR for inclusion in an Android
       application. Output file will be bin/nv-websocket-client.jar -->
  <target name="jar" depends="-compile">
    <jar destfile="bin/nv-websocket-client.jar"
         basedir="bin/classes" />
  </target>
</project>
